<icegrid>

  <application name="MidaxApp">

    <node name="nodeMidax">

      <server id="MidaxServer" exe="C:\Midax\Midax.exe" activation="on-demand">
        <adapter name="MidaxIce" endpoints="tcp -h 192.168.1.25">
          <object identity="serverController" type="::Midax::MidaxIce" property="Identity"/>
        </adapter>

        <property name="IceMX.Metrics.Debug.GroupBy" value="id"/>
        <property name="IceMX.Metrics.Debug.Disabled" value="1"/>
        <property name="IceMX.Metrics.ByParent.GroupBy" value="parent"/>
        <property name="IceMX.Metrics.ByParent.Disabled" value="1"/>
      </server>
    </node>

  </application>

</icegrid>
